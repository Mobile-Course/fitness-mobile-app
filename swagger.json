  "info": {
    "title": "Fitness App API",
    "description": "The Fitness App API description",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "SigninDto": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "email": {
            "type": "string"
          }
        },
        "required": [
          "username",
          "password",
          "name",
          "lastName"
        ]
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        },
        "required": [
          "username",
          "password"
        ]
      },
      "UpdateUserDto": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "minLength": 3
          },
          "email": {
            "type": "string",
            "format": "email"
          },
          "description": {
            "type": "string"
          },
          "sportType": {
            "type": "string"
          },
          "picture": {
            "type": "string"
          }
        }
      },
      "UpdatePreferencesDto": {
        "type": "object",
        "properties": {
          "pushNotifications": {
            "type": "boolean"
          },
          "darkMode": {
            "type": "boolean"
          },
          "units": {
            "type": "string"
          },
          "weeklyGoal": {
            "type": "number"
          }
        }
      },
      "User": {
        "type": "object",
        "properties": {}
      },
      "CreatePostDto": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "src": {
            "type": "string"
          },
          "pictures": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "workoutDetails": {
            "type": "object",
            "properties": {
              "type": {
                "type": "string"
              },
              "duration": {
                "type": "number"
              },
              "calories": {
                "type": "number"
              },
              "subjectiveFeedbackFeelings": {
                "type": "string"
              },
              "personalGoals": {
                "type": "string"
              }
            },
            "required": []
          },
          "likes": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/User"
            }
          },
          "likesNumber": {
            "type": "number"
          }
        },
        "required": [
          "title",
          "likes",
          "likesNumber"
        ]
      },
      "AddCommentDto": {
        "type": "object",
        "properties": {
          "content": {
            "type": "string"
          }
        },
        "required": [
          "content"
        ]
      },
      "CreateWorkoutSummaryDto": {
        "type": "object",
        "properties": {
          "workoutId": {
            "type": "string",
            "minLength": 1
          },
          "userId": {
            "type": "string",
            "minLength": 1
          },
          "summaryText": {
            "type": "string"
          },
          "summaryJson": {
            "type": "object",
            "propertyNames": {
              "type": "string"
            },
            "additionalProperties": {},
            "x-nestjs_zod-self-required": false
          },
          "subjectiveFeedbackFeelings": {
            "type": "string"
          },
          "personalGoals": {
            "type": "string"
          }
        },
        "required": [
          "workoutId",
          "userId"
        ]
      },
      "WorkoutSummary": {
        "type": "object",
        "properties": {}
      },
      "CreateUserProfileDto": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "minLength": 1
          },
          "profileSummaryText": {
            "type": "string",
            "maxLength": 2500
          },
          "profileSummaryJson": {
            "type": "object",
            "propertyNames": {
              "type": "string"
            },
            "additionalProperties": {},
            "x-nestjs_zod-self-required": true
          },
          "version": {
            "type": "number"
          },
          "height": {
            "type": "number",
            "exclusiveMinimum": 0
          },
          "currentWeight": {
            "type": "number",
            "exclusiveMinimum": 0
          },
          "age": {
            "type": "integer",
            "minimum": 1,
            "maximum": 120
          },
          "sex": {
            "type": "string",
            "enum": [
              "male",
              "female",
              "other"
            ]
          },
          "bodyFatPercentage": {
            "type": "number",
            "minimum": 0,
            "maximum": 100
          },
          "vo2max": {
            "type": "number",
            "exclusiveMinimum": 0
          },
          "oneRm": {
            "type": "object",
            "properties": {
              "squat": {
                "type": "number",
                "exclusiveMinimum": 0
              },
              "bench": {
                "type": "number",
                "exclusiveMinimum": 0
              },
              "deadlift": {
                "type": "number",
                "exclusiveMinimum": 0
              }
            },
            "x-nestjs_zod-self-required": false
          },
          "workoutsPerWeek": {
            "type": "integer",
            "minimum": 0,
            "maximum": 14
          }
        },
        "required": [
          "userId",
          "profileSummaryText",
          "profileSummaryJson",
          "version"
        ]
      },
      "UserProfile": {
        "type": "object",
        "properties": {}
      },
      "CreateEmbeddingDto": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "minLength": 1
          },
          "refType": {
